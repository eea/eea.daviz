<tal:view define="
  columns python:view.columns;
  labels python:view.labels">

<div ex:role="view"
  ex:viewClass="Tabular"
  ex:cellSpacing="0"
  ex:cellPadding="0"
  ex:tableStyler="DavizTableStyler"
  ex:rowStyler="DavizTableRowStyler"
  tal:condition="columns"
  tal:attributes="
    ex:columns columns;
    ex:columnLabels labels;
    "
></div>

</tal:view>
